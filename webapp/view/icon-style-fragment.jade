        url('#{url.static}/fonts/icon.woff?g43ia2') format('woff'),
        url('#{url.static}/fonts/icon.svg?g43ia2#icon') format('svg');
        font-weight: normal;
        font-style: normal;
    }

    [class^="icon-"], [class*=" icon-"] {
        /* use !important to prevent issues with browser extensions that change fonts */
